{
  "$id": "dm_directoryParents",
  "$schema": "https://schema.yext.com/config/km/field/v1",
  "displayName": "Directory Parents",
  "group": "NONE",
  "localization": "PRIMARY_ONLY",
  "type": {
    "listType": {
      "typeId": "entityReference",
      "type": {
        "entityReferenceType": {
          "supportedEntityTypeIds": ["ce_root", "ce_region", "ce_city"],
          "type": "ONE_WAY",
          "properties": {
            "name": {
              "type": "string",
              "required": true
            },
            "slug": {
              "type": "string",
              "required": true
            }
          }
        }
      }
    }
  },
  "typeId": "list",
  "required": false,
  "description": "Hierarchical reference to parent directory entities with name and slug properties.",
  "validation": {
    "maxLength": 50
  }
}
